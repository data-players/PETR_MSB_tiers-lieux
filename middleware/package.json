{
  "name": "petrmsb-middleware",
  "version": "0.1.0",
  "private": true,
  "description": "Middleware for petrmsb.org",
  "keywords": [
    "semapps",
    "semantic"
  ],
  "scripts": {
    "dev": "moleculer-runner --repl --hot services",
    "devwithlink": "make link & sleep 5 && npm run dev",
    "start": "moleculer-runner --instances=max services",
    "prestart": "npm run jwt",
    "predev": "npm run jwt",
    "jwt": "./initialize.sh"
  },
  "dependencies": {
    "@semapps/auth": "^0.3.21",
    "@semapps/backup": "0.3.21",
    "@semapps/fuseki-admin": "0.3.21",
    "@semapps/inference": "0.3.21",
    "@semapps/jsonld": "0.3.21",
    "@semapps/ldp": "0.3.21",
    "@semapps/mime-types": "0.3.21",
    "@semapps/sparql-endpoint": "0.3.21",
    "@semapps/triplestore": "0.3.21",
    "@semapps/webacl": "0.3.21",
    "@semapps/webid": "^0.3.21",
    "dotenv-flow": "^3.2.0",
    "handlebars": "^4.7.7",
    "ioredis": "^4.17.3",
    "moleculer": "^0.14.4",
    "moleculer-mail": "^1.2.5",
    "moleculer-repl": "^0.6.3",
    "moleculer-web": "0.10.0-beta1",
    "speakingurl": "^14.0.1",
    "ssh2-sftp-client": "^7.2.0",
    "url-join": "^4.0.1"
  }
}
